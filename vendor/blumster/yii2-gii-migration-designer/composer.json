    "type": "yii2-extension",
    "keywords": [ "yii2", "extension", "migration", "gii" ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Blumster",
            "email": "blumster.yii2@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "~2.0",
        "yiisoft/yii2-gii": "~2.0",
        "yiisoft/yii2-bootstrap": "~2.0"
    },
    "support": {
        "issues": "https://github.com/Blumster/yii2-gii-migration-designer/issues?state=open",
        "source": "https://github.com/Blumster/yii2-gii-migration-designer"
    },
    "autoload": {
